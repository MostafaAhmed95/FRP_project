<?xml version="1.0"?>
<robot name="myfirst">
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.3" radius="0.5"/>
      </geometry>
	<origin rpy="0 0 0" xyz="0 0 0.5"/>
    </visual>
  </link>

